{
    "name": "@datapos/datapos-shared",
    "version": "0.3.356",
    "description": "A library containing common constants, types and utilities used across all Data Positioning projects.",
    "license": "MIT",
    "author": "Jonathan Terrell <terrell.jm@gmail.com>",
    "private": false,
    "type": "module",
    "homepage": "https://www.datapos.app",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/data-positioning/datapos-shared.git"
    },
    "bugs": {
        "url": "https://github.com/data-positioning/datapos-shared/issues"
    },
    "module": "./dist/datapos-shared.es.js",
    "types": "./dist/types/src/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/datapos-shared.es.js",
            "types": "./dist/types/src/index.d.ts"
        },
        "./component/connector": {
            "import": "./dist/datapos-shared-connector.es.js",
            "types": "./dist/types/src/component/connector/index.d.ts"
        },
        "./errors": {
            "import": "./dist/datapos-shared-errors.es.js",
            "types": "./dist/types/src/errors/index.d.ts"
        },
        "./utilities": {
            "import": "./dist/datapos-shared-utilities.es.js",
            "types": "./dist/types/src/utilities/index.d.ts"
        }
    },
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "prettier": "@datapos/datapos-development/prettierrc",
    "peerDependencies": {
        "csv-parse": "^6.1.0",
        "date-fns": "^4.1.0",
        "nanoid": "^5.1.6"
    },
    "devDependencies": {
        "@datapos/datapos-development": "^0.3.424",
        "@datapos/eslint-config-datapos": "^1.0.28",
        "@types/node": "^25.0.3",
        "@typescript-eslint/eslint-plugin": "^8.50.0",
        "@typescript-eslint/parser": "^8.50.0",
        "eslint": "^9.39.2",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-security": "^3.0.1",
        "eslint-plugin-sonarjs": "^3.0.5",
        "eslint-plugin-unicorn": "^62.0.0",
        "jiti": "^2.6.1",
        "license-downloader": "^1.3.3",
        "license-report": "^6.8.1",
        "license-report-check": "^0.1.2",
        "license-report-recursive": "^6.8.2",
        "md-to-pdf": "^5.2.5",
        "nanoid": "^5.1.6",
        "npm-check-updates": "^19.2.0",
        "owasp-dependency-check": "^1.0.0",
        "prettier": "^3.7.4",
        "rollup-plugin-visualizer": "^6.0.5",
        "sonda": "^0.10.1",
        "type-fest": "^5.3.1",
        "typedoc": "^0.28.15",
        "typedoc-github-theme": "^0.3.1",
        "typedoc-material-theme": "^1.4.1",
        "typescript": "^5.9.3",
        "valibot": "^1.2.0",
        "vite": "^7.3.0",
        "vite-plugin-dts": "^4.5.4"
    },
    "scripts": {
        "audit": "op run --env-file=.env -- node -e \"import('@datapos/datapos-development').then(m => m.auditDependencies())\"",
        "build": "node -e \"import('@datapos/datapos-development').then(m => m.buildProject())\"",
        "check": "node -e \"import('@datapos/datapos-development').then(m => m.checkDependencies())\"",
        "document": "op run --env-file=.env -- node -e \"import('@datapos/datapos-development').then(m => m.documentDependencies(['MIT']))\"",
        "format": "node -e \"import('@datapos/datapos-development').then(m => m.formatCode())\"",
        "lint": "node -e \"import('@datapos/datapos-development').then(m => m.lintCode())\"",
        "release": "op run --env-file=.env -- node -e \"import('@datapos/datapos-development').then(m => m.releaseProject())\"",
        "sync": "node -e \"import('@datapos/datapos-development').then(m => m.syncProjectWithGitHub())\"",
        "test": "node -e \"import('@datapos/datapos-development').then(m => m.testProject())\"",
        "update": "node -e \"import('@datapos/datapos-development').then(m => m.updateDataPosDependencies(['development', 'eslint']))\"",
        "_readmeToPDF": "md-to-pdf README.md",
        "_td": "npx typedoc --options docs/typedoc.json",
        "_td:github": "npx typedoc --options docs/typedoc.json --plugin typedoc-github-theme",
        "_td:material": "npx typedoc --options docs/typedoc.json  --plugin typedoc-material-theme --themeColor '#cb9820'"
    },
    "engines": {
        "node": ">=22.0.0",
        "npm": ">=11.0.0"
    }
}